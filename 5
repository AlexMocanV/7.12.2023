#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a,b;
    cin>>a>>b;
    if(a==0&&b==0)
    {
        cout<<"punctul se afla la origine";
    }
    else if(a>=0&&b>=0)
    {
        cout<<1;
    }
    else if(a>=0&&b<=0)
    {
        cout<<4;
    }
    else if(a<=0&&b<=0)
    {
        cout<<3;
    }
    else if(a<=0&&b>=0)
    {
        cout<<2;
    }
    else if(a==0&&b!=0)
    {
        cout<<"punctul se afla pe axa Ox";
    }
    else if(a!=0&&b==0)
    {
        cout<<"punctul se afla pe axa Oy";
    }
    return 0;
}
